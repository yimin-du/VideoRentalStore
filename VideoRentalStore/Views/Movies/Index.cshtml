@model IEnumerable<VideoRentalStore.Models.Movie>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies</h2>
<table class="table table-striped table-bordered">
    <thead>
        <tr><th>Movie</th></tr>
    </thead>
    <tbody>
        @foreach (var movie in Model)
        {
            <tr>
                <td>@movie.Name</td >
            </tr>
        }
    </tbody>
   
</table>
